    <configMap currentDirectory="..\Out">
      <add key="currentDirectoryKey1" value="currentDirectoryValue1" />
    </configMap>

    <configMap currentDirectoryRegEx=".*\\TestResults">
      <add key="currentDirectoryRegExKey1" value="currentDirectoryRegExValue1" />
